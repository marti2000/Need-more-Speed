<Window x:Class="Need_more_Speed.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Need_more_Speed"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <Canvas Name="racingtrack" Background="Green">
            <Line Canvas.Left="25" Canvas.Top="25" Height="400" Width="1000" Stroke="Gray" />
            <Rectangle Name="road" Width="100" Height="100" Fill="Gray" Canvas.Left="10" Canvas.Top="100"/>
        </Canvas>
    </Grid>
</Window>
